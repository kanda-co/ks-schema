  "dependencies": {
    "@kanda-libs/ks-design-library": "^0.0.47",
    "@kanda-libs/ks-tailwind-config": "^0.0.5",
    "@types/lodash.get": "^4.4.7",
    "lodash.get": "^4.4.2",
    "react-hook-form": "^7.35.0",
    "react-input-autosize": "^3.0.0",
