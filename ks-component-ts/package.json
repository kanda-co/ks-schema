{
  "name": "@kanda-libs/ks-component-ts",
  "version": "0.1.8-postcode-5",
  "description": "Kanda form components library",
  "main": "dist/index.cjs.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    ""
  ],
  "scripts": {
    "clean": "node_modules/.bin/rimraf dist",
    "prebuild": "yarn clean",
    "build": "yarn build:style && yarn build:modern",
    "build:modern": "node_modules/.bin/rollup -c ./scripts/rollup/rollup.config.js",
    "watch:modern": "yarn build:modern -w",
    "build:esm": "node_modules/.bin/rollup -c ./scripts/rollup/rollup.esm.config.js",
    "build:style": "npx tailwindcss --postcss ./tailwind.postcss.config.js -i ./src/styles/tailwind.css -c ./tailwind.config.js -o ./src/styles/library.css",
    "watch:style": "yarn build:style --watch",
    "lint": "node_modules/.bin/eslint '**/*.{js,ts,tsx}'",
    "type": "node_modules/.bin/tsc --noEmit",
    "bundlesize": "yarn build:style && yarn build:modern && node_modules/.bin/bundlesize",
    "start:app": "yarn clean && node_modules/.bin/tsc && yarn build:style && yarn build:esm && yarn link && yarn --cwd node_modules/react link && yarn --cwd ./app link react @kanda-libs/ks-form-library-2 && yarn --cwd ./app && yarn --cwd ./app run dev"
  },
  "author": "rob@kanda.co.uk",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^17.0.29",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-input-autosize": "^2.2.1",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.5",
    "bundlesize": "^0.18.1",
    "clsx": "^1.1.1",
    "dts-bundle-generator": "^6.9.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "esm": "^3.2.25",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.12",
    "postcss-100vh-fix": "^1.0.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading-skeleton": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-shell": "^1.0.3",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "tailwindcss": "^3.0.24",
    "tailwindcss-invalid-variant-plugin": "^1.0.3",
    "tsd": "^0.19.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "bundlesize": [
    {
      "path": "./dist/index.cjs.js",
      "maxSize": "10000 kB"
    }
  ],
  "peerDependencies": {
    "clsx": "^1.1.1",
    "lodash.get": "^4.4.2",
    "react": "^16.8.0 || ^17 || ^18",
    "react-hook-form": "^7.27.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kanda-co/ks-form-library.git"
  },
  "keywords": [
    "design"
  ],
  "bugs": {
    "url": "https://github.com/kanda-co/ks-design-library/issues"
  },
