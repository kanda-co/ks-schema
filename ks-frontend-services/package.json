{
  "name": "@kanda-libs/ks-frontend-services",
  "version": "0.2.0",
  "license": "private",
  "scripts": {
    "clean": "rimraf dist",
    "generate-service-module": "./node_modules/.bin/ts-node ./src/service/generator/",
    "build:types": "./node_modules/.bin/dts-bundle-generator --external-inlines=./generated -o src/generated/dist/index.d.ts src/index.ts",
    "build": "yarn clean && yarn generate-service-module && yarn build:types && node ./build.ts",
    "test:app": "yarn clean && yarn && yarn build && cd app && yalc update && cd .. && yarn --cwd ./app && yarn --cwd ./app dev --force"
  },
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@openapi-io-ts/runtime": "^0.3.1",
    "@stripe/stripe-js": "^1.35.0",
    "append-query": "^2.1.1",
    "firebase": "^9.9.3",
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.19",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "dts-bundle-generator": "^7.1.0",
    "esbuild": "^0.15.16",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-node-externals": "^1.5.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@react-pdf/font": "2.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
