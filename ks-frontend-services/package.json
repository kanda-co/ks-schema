{
  "name": "@kanda-libs/ks-frontend-services",
  "version": "0.2.51",
  "license": "private",
  "scripts": {
    "clean-incorrect-imports": "grep -rlw \"./src\" -e \"DateFromISOString\" | xargs -I@ sed -i.bak \"s/\\/DateFromISOString//g\" @ && rm -rf **/*.bak",
    "generate-service-module": "yarn clean-incorrect-imports && ts-node --experimental-specifier-resolution=node ./src/service/generator/",
    "clean": "rimraf dist",
    "remove:nodemodules": "rimraf yarn.lock && rimraf node_modules",
    "build:types": "tsc src/index.ts --declaration --emitDeclarationOnly --jsx react-jsx --esModuleInterop",
    "build": "yarn && yarn clean && yarn generate-service-module && yarn build:types && ts-node ./build.ts",
    "watch": "yarn && yarn clean && DEV_WATCH=true yarn build",
    "app:dev": "yarn --cwd ./app && yarn --cwd ./app yalc:link && yarn --cwd ./app dev --force --host",
    "test:app": "yarn clean && yarn && yarn build && yalc publish && yarn --cwd ./app yalc:update && yarn --cwd ./app && yarn --cwd ./app dev --force"
  },
  "main": "dist/index.esm.js",
  "types": "src/index.d.ts",
  "module": "dist/index.esm.js",
  "type": "module",
  "files": [
    "dist",
    "src/**/*.d.ts"
  ],
  "dependencies": {
    "@kanda-libs/ks-amplitude-provider": "^0.0.15",
    "@openapi-io-ts/runtime": "^0.3.1",
    "@stripe/stripe-js": "^1.35.0",
    "append-query": "^2.1.1",
    "firebase": "^9.9.3",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.19",
    "remove": "^0.1.5",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "esbuild": "^0.15.16",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-node-externals": "^1.5.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "yalc": "^1.0.0-pre.53"
  },
  "resolutions": {
    "@react-pdf/font": "2.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
